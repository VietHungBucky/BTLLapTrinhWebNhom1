<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<style>
    div {
        margin-top: 50px;
        text-align: center;
    }

    table {
        width: 100%;
        padding-left: 300px;
        padding-right: 300px;
    }

    td,
    th {
        border: 2px solid #dddddd;
        text-align: left;
        padding: 8px;
    }

    tr:nth-child(even) {
        background-color: #dddddd;
    }
</style>

<body>
    <div className="content">
        <h1>WELCOME TO BACKEND PAGE</h1>
        <p style="font-size: 25px;">List admin</p>
        <table>
            <tr>
                <th>ID</th>
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Address</th>
                <th>Phone</th>
            </tr>

            <% for (var i=0; i < users.length; i++) { %>
                <tr>
                    <td>
                        <%= users[i].id %>
                    </td>
                    <td>
                        <%= users[i].email %>
                    </td>
                    <td>
                        <%= users[i].firstName %>
                    </td>
                    <td>
                        <%= users[i].lastName %>
                    </td>
                    <td>
                        <%= users[i].address %>
                    </td>
                    <td>
                        <%= users[i].phone %>
                    </td>
                </tr>
                <% } %>
        </table>

    </div>
</body>

</html>